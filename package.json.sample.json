{
  "name": "ImportSPA",
  "version": "1.0.0",
  "description": "SPA for Import",
  "author": "CARiD",
  "repository": {
    "type": "git",
    "url": "ssh://git@bitbucket.onyxenterprises.com:7999/imp/frontend.git"
  },
  "license": "ISC",
  "scripts": {
    "start": "concurrently \"npm run start-dev\"",
    "start-dev": "cross-env NODE_ENV=development webpack-dev-server --hot --inline",
    "start-prod": "cross-env NODE_ENV=production webpack-dev-server --hot --inline",

    "clear-dist": "rm -rf dist-*/",
    "generate-prod": "cross-env NODE_ENV=production webpack --progress",
    "generate-prod1": "cross-env NODE_ENV=production_masterdev webpack --progress",
    "generate-prod2": "cross-env NODE_ENV=production_masterweekly webpack --progress",
    "generate-prod3": "cross-env NODE_ENV=production_stagedev webpack --progress",
    "generate-prod4": "cross-env NODE_ENV=production_stageweekly webpack --progress",
    "generate-prod5": "cross-env NODE_ENV=production_testdev webpack --progress",
    "generate-prod6": "cross-env NODE_ENV=production_testweekly webpack --progress",
    "build-prod-aggregated": "npm run clear-dist && npm run generate-prod && npm run generate-prod1 && npm run generate-prod2 && npm run generate-prod3 && npm run generate-prod4 && npm run generate-prod5 && npm run generate-prod6",
    "build-prod": "npm test && npm run build-prod-aggregated",

    "clear-prod-dist": "rm -rf dist-production/",
    "build-prod-only": "npm run clear-prod-dist && npm run generate-prod",

    "lint-src": "eslint src/",
    "lint-test": "eslint test/",
	"mocha-tests": "mocha \"./test/*.test.js\" --compilers js:babel-core/register",
	"test": "npm run lint-src && npm run lint-test && npm run mocha-tests"
  },
  "devDependencies": {
    "babel-core": "^6.17.0",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.16.0",
    "bootstrap-sass": "^3.3.7",
    "concurrently": "^2.2.0",
    "cross-env": "^2.0.1",
    "css-loader": "^0.23.1",
    "eslint": "^3.8.1",
    "eslint-plugin-react": "^5.2.2",
    "expect": "^1.20.2",
    "expect-jsx": "^2.6.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.24.0",
    "mocha": "^3.1.2",
    "node-sass": "^3.10.1",
	"react-addons-test-utils": "^15.3.2",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.2"
  },
  "dependencies": {
    "axios": "^0.13.1",
    "chart.js": "^1.1.1",
    "immutable": "^3.8.1",
    "react": "^15.3.2",
    "react-bootstrap": "^0.30.5",
    "react-bootstrap-date-picker": "^3.7.0",
    "react-chartjs": "^0.8.0",
    "react-dom": "^15.3.2",
    "react-json-pretty": "^1.2.1",
    "react-redux": "^4.4.5",
    "react-router": "^2.8.1",
    "react-select": "1.0.0-rc.2",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0"
  }
}